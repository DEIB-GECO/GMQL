<?xml version = "1.0" encoding="UTF-8" standalone="yes"?>
<gmqlProperties xmlns = "http://genomic.elet.polimi.it/impl">
    <property name = "SPARK_HOME">/usr/local/spark/</property>
    <property name = "CLI_JAR_NAME">GMQL-Cli-2.0-jar-with-dependencies.jar</property>
    <property name = "CLI_CLASS">it.polimi.genomics.cli.GMQLExecuteCommand</property>
    <property name = "LAUNCHER_MODE">LOCAL</property>
    <property name = "LIB_DIR_LOCAL">../lib/</property>
    <property name = "LIB_DIR_HDFS">/user/repo/lib/</property>
    <property name = "SPARK_UI_PORT">4050</property>

    <property name="SPARK_CUSTOM" user-category="GUEST"  spark-property="spark.cores.max">6</property>
    <property name="SPARK_CUSTOM" user-category="BASIC"  spark-property="spark.cores.max">12</property>
    <property name="SPARK_CUSTOM" user-category="PRO"    spark-property="spark.cores.max">15</property>
    <property name="SPARK_CUSTOM" user-category="ADMIN"  spark-property="spark.cores.max">18</property>
    <property name="SPARK_CUSTOM" user-category="PUBLIC" spark-property="spark.cores.max">18</property>
